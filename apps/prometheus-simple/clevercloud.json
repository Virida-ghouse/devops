{
  "build": {
    "type": "python",
    "buildCommand": "pip install -r requirements.txt"
  },
  "deploy": {
    "target": "prometheus-simple",
    "command": "python app.py"
  },
  "environment": {
    "PYTHON_VERSION": "3.11",
    "CC_PYTHON_MODULE": "app:app",
    "PROMETHEUS_PORT": "9090"
  }
}
